pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--main
t=0
world_max_x=512
world_max_y=512
star_prob=0.001


player={}
bullets={}
planets={}
enemies={}
bg_sprites={}

active_entities={}
draw_table={}

function _init()
 player=player:new{player}
 player:init()
 add(active_entities,player)
 camera_pos={x=0,y=0}
 generate_background()
 local planet=create_planet(128,128)
 add(active_entities,planet)
 add(draw_table,planet)
 add(draw_table,player)
end

function _update()
 t+=1
 for entity in all(active_entities) do
  entity:update()
 end
 update_camera()
end

function _draw()
 cls()
 camera(camera_pos.x,camera_pos.y)
 draw_world()
 for entity in all(draw_table) do
  entity:draw()
 end
 camera(0,0)
 draw_hud()
end

function update_camera()
 camera_pos.x=player.x-60
 camera_pos.y=player.y-60
end

function draw_hud()
 for i=1,player.max_hp do
  spr(5,9*i-5,12)
 end
  
 for i=1,player.hp do
  spr(4,9*i-5,12)
 end
end


function draw_world()
 --world border  
 rect(0,0,world_max_x,world_max_y,color(6))
 --background
 for sprite in all(bg_sprites) do
  pset(sprite.x,sprite.y,color(7))
 end
end

function generate_background()
 for x=1,world_max_x-1 do
  for y=1,world_max_y-1 do
   if rnd()<star_prob then
    add(bg_sprites,{x=x,y=y})
   end
  end
 end
end

function game_over()
 run()
end

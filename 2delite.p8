--planets
local planet={}

function planet:new (o)
 o = o or {}   -- create object if user does not provide one
 setmetatable(o, self)
 self.__index = self
 return o
end

function create_planet(x,y)
 local p=planet:new{x=x,y=y,w=32,h=32}
 p.kind=1
 p.has_player=false
 p.cooldown=0
 p.hitbox={x=x+11,y=y+11,w=10,h=10}
 p.fuel_tank=p.max_fuel
 add(planets,p)
 return p
end

function planet:draw()
  if (self.has_player) pal(11,8)
  spr(64,self.x,self.y,4,4)
  pal()
end

function planet:update()
 if self.has_player then
  
  if (t%2 == 0 and fuel<max_fuel) fuel+=1
 end
  
end

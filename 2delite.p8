--player
player.x=60
player.y=60
player.aim=.5
player.r=3
player.tip={}
player.br={}
player.br.x = 0
player.br.y = 0
player.bl={}
player.bl.x = 0
player.bl.y = 0
player.tip.x = 0
player.tip.y = 0
player.hp=0
player.max_hp=3
player.dx=0
player.dy=0
player.rotspeed=.025
player.accelpow=.15
player.is_accelerating=false
player.max_speed=3
player.shooting_cd_counter=0
player.shooting_cd=15
player.fuel=0
player.max_fuel=512
player.state=0

function player:new (o)
 o = o or {}
 setmetatable(o, self)
 self.__index = self
 return o
end

function player:init()
 self.fuel=self.max_fuel
 self.x=128
 self.y=128
 self.hp=self.max_hp
 self.aim=.5
 self.dx=0
 self.dy=0
 self.color=7
 self.state=1
end

function player:update_pos()
 self.tip.x = (sin(self.aim)*self.r)+self.x
 self.tip.y = (cos(self.aim)*self.r)+self.y
 self.br.x = (sin(self.aim+.39)*self.r)+self.x
 self.br.y = (cos(self.aim+.39)*self.r)+self.y
 self.bl.x = (sin(self.aim-.39)*self.r)+self.x
 self.bl.y = (cos(self.aim-.39)*self.r)+self.y
 self.x+=player.dx
 self.y+=player.dy
end

function player:update()
 local previous_dx=self.dx
 local previous_dy=self.dy
 self:update_pos()
 
 if self.state==0 then
  game_over()
 elseif self.state==1 then 
  if self.x<=0 then
   self.x=0
   self.dx=-self.dx/2
   sfx(2)
  elseif self.x>=world_max_x then
   self.x=world_max_x
   self.dx=-self.dx/2
  end
  
  if self.y<=0 then
   self.y=0
   self.dy=-self.dy/4
  elseif self.y>=world_max_y then
   self.y=world_max_y
   self.dy=-self.dy/4
  end
  
  if self.shooting_cd_counter>0 then
   self.shooting_cd_counter-=1
  end

  if btn(⬅️) then
   self.aim-=self.rotspeed
  end
  if btn(➡️) then
   self.aim+=self.rotspeed
  end
  if btn(⬆️) and self.fuel>0 then
   self.fuel-=1
   self.dx+=-sin(self.aim*-1)*self.accelpow
   self.dy+=cos(self.aim*-1)*self.accelpow
   if abs(self.dx) > self.max_speed then
    if self.dx > 0 then
     self.dx=self.max_speed
    else
     self.dx=-self.max_speed
    end 
   end
    
    if abs(self.dy) > self.max_speed then
     if self.dy > 0 then
      self.dy=self.max_speed
     else
      self.dy=-self.max_speed
     end
    end
    if self.dx!=previous_dx or self.dy!=previous_dy then
     self.is_accelerating=true
     sfx(0)
    end
  else
   self.is_accelerating=false
  end
  
  if btn(⬇️) and self.fuel>1 then
   self.fuel-=2
   if (self.dx>0 or self.dy>0) then sfx(0) end
    
    if self.dx>0 then
     if self.dx-self.accelpow<=0 then
      self.dx=0
     else
      self.dx-=self.accelpow
     end
      
    elseif self.dx<0 then
     if self.dx+self.accelpow>=0 then
      self.dx=0
     else
      self.dx+=self.accelpow
     end
    end
      
    if self.dy>0 then
     if self.dy-self.accelpow<=0 then
      self.dy=0
     else
      self.dy-=self.accelpow
     end
      
    elseif self.dy<0 then
     if self.dy+self.accelpow>=0 then
      self.dy=0
     else
      self.dy+=self.accelpow
     end
    end
    
    if self.dx!=previous_dx or self.dy!=previous_dy then
     self.is_accelerating=true
     sfx(0)
    else
     self.is_accelerating=false
    end
    
  end
  
  if btn(4) then
   if self.shooting_cd_counter==0 then
  	 self:fire_bullet()
  	 self.shooting_cd_counter=self.shooting_cd
   end
  end
  
  if (self.fuel==0 or self.hp==0) self.state=0
  
 elseif self.state==2 then
  
 end
end

function player:draw()
 line(self.tip.x, self.tip.y, self.br.x, self.br.y, self.color)
 line(self.tip.x, self.tip.y, self.bl.x, self.bl.y, self.color)
 if self.is_accelerating then
  line(self.bl.x, self.bl.y, self.br.x, self.br.y, color(9))
 else
  line(self.bl.x, self.bl.y, self.br.x, self.br.y, self.color)
 end
 --displays velocity vectors
 --line(self.tip.x,self.tip.y, self.tip.x+self.dx*5, self.tip.y,color(10))
 --line(self.tip.x,self.tip.y, self.tip.x, self.tip.y+self.dy*5,color(10))
end

function player:fire_bullet()
 if #bullets<5 then
  sfx(1)
  local bullet = {}
  bullet.shooter=self
  bullet.color=self.color
  bullet.dx=-sin(self.aim*-1)*bullet_spd
  bullet.dy=cos(self.aim*-1)*bullet_spd
  bullet.x=self.tip.x
  bullet.y=self.tip.y
  bullet.t=30
  add(bullets,bullet)
 end
end

function player:check_collision()
 for entity in all(draw_table) do
  if (player:is_inside(entity)) resolve_collision(entity)   
 end
end

function player:is_inside(entity)
 return point_inside(player.tip,entity)
    and point_inside(player.bl,entity)
    and point_inside(player.br,entity)
end
